        <script type="text/javascript">
            var timer;
            //var compareDate = new Date();
            //compareDate.setDate(compareDate.getDate()); //just for this demo today + 7 days
            //timer = setInterval(function() {
            //    timeBetweenDates(compareDate);
            //}, 1000);

            function timeBetweenDates(toDate) {
                var dateEntered = toDate;
                var now = new Date();
                var difference = dateEntered.getTime() - now.getTime();

                if (difference <= 0) {
                    clearInterval(timer);

                } else {

                    var seconds = Math.floor(difference / 1000);
                    var minutes = Math.floor(seconds / 60);
                    var hours = Math.floor(minutes / 60);
                    var days = Math.floor(hours / 24);

                    hours %= 24;
                    minutes %= 60;
                    seconds %= 60;

                    $("#days").text(days);
                    $("#hours").text(hours);
                    $("#minutes").text(minutes);
                    $("#seconds").text(seconds);
                }
            }

            function SetTimerForPosition(date)
            {
                var Newdate = new Date(date);
                Newdate.setDate(Newdate.getDate());
                if (timer != null) {
                    clearInterval(timer);
                }
                timer = setInterval(function() {
                    timeBetweenDates(Newdate);
                }, 1000);
            }

            function GetTimeForPosition(n)
            {
                switch (parseInt(n)) {
                    case 0:
                        SetTimerForPosition("Jan 31, 2018 23:59:59");
                        $("#timer").show();
                        break;
                    case 1:
                        var t= new Date();
                        var m = new Date(t.getFullYear(), t.getMonth() + 1, 0, 22, 00, 00);
                        SetTimerForPosition(m);
                        $("#timer").show();
                        break;
                    case 2:
                        var end = new Date();
                        end.setHours(23,59,59,999);
                        SetTimerForPosition(end);
                        $("#timer").show();
                        break;
                    case 3:
                        //var curr = new Date;
                        //var first = curr.getDate() - curr.getDay();
                        //var last = first + 6;
                        var sunday = moment().day(7);
                        SetTimerForPosition(sunday);
                        $("#timer").show();
                        break;
                    case 4:
                        var today = new Date();
                        var lastDayOfMonth = new Date(today.getFullYear(), today.getMonth()+1, 0);
                        SetTimerForPosition(lastDayOfMonth);
                        $("#timer").show();
                        break;
                    case 5:
                        var lastday = new Date(new Date().getFullYear(), 11, 31);
                        SetTimerForPosition(lastday);
                        $("#timer").show();
                        break;
                    case 6:
                        clearInterval(timer);
                        $("#timer").hide();
                        break;
                    case 7:
                        clearInterval(timer);
                        $("#timer").hide();
                        break;
                    default:
                }
            }
        </script>
