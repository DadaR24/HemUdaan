            function PreviewImage(elementID, imgElement) {
                var oFReader = new FileReader();
                oFReader.readAsDataURL(document.getElementById(elementID).files[0]);
                oFReader.onload = function (oFREvent) {
                    document.getElementById(imgElement).src = oFREvent.target.result;
                };
                var FileType = null;
                var image = "";
                $("#imageData").val("");
                var filesSelected = document.getElementById(elementID).files;
                if (filesSelected.length > 0) {
                    var fileToLoad = filesSelected[0];
                    FileType = filesSelected[0].type;
                    fileToLoad.convertToBase64(function (base64) {
                        if (FileType == "image/jpg") {
                            image = base64.replace('data:image/jpg;base64,', '');
                        }
                        else if (FileType == "image/png") {
                            image = base64.replace('data:image/png;base64,', '');
                        }
                        else if (FileType == "image/jpeg") {
                            image = base64.replace('data:image/jpeg;base64,', '');
                        }
                        else if (FileType == "image/gif") {
                            image = base64.replace('data:image/gif;base64,', '');
                        }
                        $("#imageData").val(image);
                    });
                }
            };

            File.prototype.convertToBase64 = function (callback) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    callback(e.target.result)
                };
                reader.onerror = function (e) {
                    callback(null);
                };
                reader.readAsDataURL(this);
            };
