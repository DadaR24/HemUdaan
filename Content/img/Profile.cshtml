            function FileType(FileName)
            {
                var Type = null;
                if (IsValidExtension(FileName, ['mp4', 'avi', 'mpg', 'mpeg', 'm4v', '3gp', 'flv'])) {
                    Type = "Video";
                    return Type;
                }
                if (IsValidExtension(FileName, ['doc', 'docx', 'pdf', 'xlsx', 'xls', 'txt' ,'rtf', 'ppt', 'pptx'])) {
                    Type = "Doc";
                    return Type;
                }
                if (IsValidExtension(FileName, ['jpg', 'jpeg', 'png', 'bmp', 'gif'])) {
                    Type = "Image";
                    return Type;
                }
                if (IsValidExtension(FileName, ['mp3', 'wav', 'acc', 'mdb'])) {
                    Type = "Audio";
                    return Type;
                }

                if (IsValidExtension(FileName, ['rar', 'zip', '7zip', 'tgz'])) {
                    Type = "Resources";
                    return Type;
                }

                if (IsValidExtension(FileName, ['html', 'htm', 'php', 'js', 'css'])) {
                    Type = "Web";
                    return Type;
                }
            }

            function IsValidExtension(stringToCheck, acceptableExtensionsArray) {
                if (stringToCheck.length == 0) { return false; }
                for (var i = 0; i < acceptableExtensionsArray.length; i++) {
                    if (stringToCheck.toLowerCase().endsWith(acceptableExtensionsArray[i].toLowerCase())) { return true; }
                }
                return false;
